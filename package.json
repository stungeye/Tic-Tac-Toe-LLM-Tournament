{
  "name": "tictacgo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "node --loader ts-node/esm src/main.ts",
    "tournament": "tsx src/main.ts",
    "list-models": "tsx src/list-models.ts",
    "analyze": "tsx src/analyze-results.ts",
    "clear-logs": "rm -rf logs",
    "archive-logs": "if [ -d logs ]; then tar -czf \"logs-archive-$(date +%Y%m%d-%H%M%S).tar.gz\" logs && echo \"Archived logs to logs-archive-$(date +%Y%m%d-%H%M%S).tar.gz\"; else echo \"No logs directory to archive\"; fi"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.2.4"
  },
  "dependencies": {
    "fs-extra": "^11.3.2",
    "openai": "^6.9.1"
  }
}
